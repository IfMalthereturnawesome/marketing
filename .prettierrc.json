{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames",
    "prettier-plugin-merge"
  ],
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 110,
  "singleAttributePerLine": true,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf"

}
